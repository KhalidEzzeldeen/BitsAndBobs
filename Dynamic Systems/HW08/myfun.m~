function F = myfun(x)

% x0 = [(pi+0.1)/4; (3*pi+0.1)/4; -2; -1];
% options=optimset('MaxFunEvals', 5000, 'TolFun',1e-8, 'TolX', 1e-8, 'MaxIter', 5000);
% [xSol,fval] = fsolve(@myfun,x0,options)
u = 1;
%  uf =1;
xf = inline('(k/2)*sin(2*t) + (j - .5*u)*cos(2*t) + .5*u', 't', 'u', 'k', 'j');
vf = inline('k*cos(2*t) - (2*j - u)*sin(2*t)', 't', 'u','k','j');
x1f = inline('k*sin(2*t) + j*cos(2*t) + .5*u', 't', 'u', 'k', 'j');
v1f= inline('2*k*cos(2*t) - 2*j*sin(2*t)', 't', 'u','k','j');

% %  2-switch
% F = [xf(x(1),u,1.1,3.3) - x1f(x(1),-u,x(3),x(4));...
%      vf(x(1),u,1.1,3.3) - v1f(x(1),-u,x(3),x(4));...
%      x1f(x(1)+pi/2,-u,x(3),x(4)) - x1f(x(1)+pi/2,u,x(5),x(6));...
%      v1f(x(1)+pi/2,-u,x(3),x(4)) - v1f(x(1)+pi/2,u,x(5),x(6));...
%      x1f(x(2),u,x(5),x(6));...
%      v1f(x(2),u,x(5),x(6))];
  
% %  3-switch
% F = [xf(x(1),u,1.1,3.3) - x1f(x(1),-u,x(3),x(4));...
%      vf(x(1),u,1.1,3.3) - v1f(x(1),-u,x(3),x(4));...
%      x1f(x(1)+pi/2,-u,x(3),x(4)) - x1f(x(1)+pi/2,u,x(5),x(6));...
%      v1f(x(1)+pi/2,-u,x(3),x(4)) - v1f(x(1)+pi/2,u,x(5),x(6));...
%      x1f(x(1)+pi,u,x(5),x(6)) - x1f(x(1)+pi,-u,x(7),x(8));...
%      v1f(x(1)+pi,u,x(5),x(6)) - v1f(x(1)+pi,-u,x(7),x(8));...
%      x1f(x(2),-u,x(7),x(8));...
%      v1f(x(2),-u,x(7),x(8))];
 
%  4-switch
F = [xf(x(1),u,1.1,3.3) - x1f(x(1),-u,x(3),x(4));...
     vf(x(1),u,1.1,3.3) - v1f(x(1),-u,x(3),x(4));...
     x1f(x(1)+pi/2,-u,x(3),x(4)) - x1f(x(1)+pi/2,u,x(5),x(6));...
     v1f(x(1)+pi/2,-u,x(3),x(4)) - v1f(x(1)+pi/2,u,x(5),x(6));...
     x1f(x(1)+pi,u,x(5),x(6)) - x1f(x(1)+pi,-u,x(7),x(8));...
     v1f(x(1)+pi,u,x(5),x(6)) - v1f(x(1)+pi,-u,x(7),x(8));...
     x1f(x(1)+(3/2)*pi,-u,x(7),x(8)) - x1f(x(1)+(3/2)*pi,u,x(9),x(10));...
     v1f(x(1)+(3/2)*pi,-u,x(7),x(8)) - v1f(x(1)+(3/2)*pi,u,x(9),x(10));...
     x1f(x(2),u,x(9),x(10));...
     v1f(x(2),u,x(9),x(10))];
 
% x0 = [(pi-1)/4; 2.5*pi-1; .4; 2.3; .33; 1.5; .25; .9; .2; .5];
% options=optimset('MaxFunEvals', 5000, 'TolFun',1e-8, 'TolX', 1e-8,
% 'MaxIter', 5000);