function [t,s] = ChaosPlot(tspan,g,l)

xin = [pi/18,pi/12,pi/6,pi/3;0,0,0,0];
options = [];
ii = 2;

[t,s] = ode45(@odetest, tspan, xin(:,4), options,ii, g,l);


function xprime = odetest(~,x,ii,g,l)
% x(1) = x
% x(2) = y

switch ii
    case 1
        gp = @(xin) -g*l*sin(xin);
    case 2
        gp = @(xin) -g*l*xin;
    
end

xprime(1) = x(2);
xprime(2) = gp(x(1));
xprime = xprime(:);






TestIndex = (1:50:7090)+100;
plot(s(TestIndex,1),s(TestIndex,2),'LineStyle', 'none', 'Marker', 'o',...
          'MarkerFaceColor', 'r', 'MarkerEdgeColor', 'r', 'MarkerSize', 9);
title(['\fontsize{16} Phase Plot for Pendulum, Selected Times Quad 1, Starting Angle:  ' sAng{kk} '    ']);
